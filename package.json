{
	"name": "@nuogz/postgresql",
	"version": "1.2.0",
	"description": "postgresql library based on pg",
	"author": "DanoR",
	"license": "LGPL-3.0-or-later",
	"repository": {
		"type": "git",
		"url": "github:nuogz/postgresql"
	},
	"private": false,
	"type": "module",
	"typesSource": [
		"node"
	],
	"sideEffects": false,
	"engines": {
		"node": ">=14.18.1"
	},
	"main": "./index.js",
	"files": [
		"index.js",
		"lib/**",
		"locale/**",
		"**/*.d.ts"
	],
	"typings": "./index.d.ts",
	"scripts": {
		"emitDeclaration": "tsc ./index.js --declaration --emitDeclarationOnly --moduleResolution bundler --module esnext --target esnext --experimentalDecorators --strict --allowJs --jsx preserve"
	},
	"dependencies": {
		"@nuogz/i18n": "^3.3.1",
		"@nuogz/utility": "^1.8.1",
		"pg": "^8.11.3"
	},
	"devDependencies": {
		"@types/pg": "^8.10.9",
		"eslint": "^8.55.0",
		"typescript": "^5.3.3"
	}
}
